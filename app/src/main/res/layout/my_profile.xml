<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/white">

        <ImageView
            android:id="@+id/img_toolbar"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:gravity="start"
            app:srcCompat="@drawable/ic_logout" />

        <TextView
            android:id="@+id/txt_toolbar"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_gravity="center"
            android:fontFamily="@font/cairobold"
            android:text="@string/Profile"
            android:textColor="@color/blackcolor1"
            android:textSize="20dp" />

        <ImageView
            android:id="@+id/edit_icon"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_gravity="end"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="15dp"
            app:srcCompat="@drawable/ic_edit_profile" />
    </androidx.appcompat.widget.Toolbar>
    <View
        android:id="@+id/view969"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/blackcolor" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView26"
                    android:layout_width="77dp"
                    android:layout_height="77dp"
                    android:layout_gravity="start"
                    android:layout_marginStart="16dp"
                    android:scaleType="fitXY"
                    app:srcCompat="@drawable/imageee" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_marginEnd="16dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/textView38"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:layout_marginTop="10dp"
                        android:fontFamily="@font/cairobold"
                        android:text="@string/name11"
                        android:textColor="@color/blackcolor1"
                        android:textSize="15sp"
                        android:textAlignment="viewStart"/>

                    <TextView
                        android:id="@+id/textView39"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:fontFamily="@font/cairoregular"
                        android:gravity="start"
                        android:text="@string/email1"
                        android:textSize="13dp" />
                </LinearLayout>
            </LinearLayout>

              <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/my_order_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        app:srcCompat="@drawable/ic_sell_your_product" />

                    <TextView
                        android:id="@+id/my_order"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="10dp"
                        android:textColor="@color/my_profile"
                        android:fontFamily="@font/cairosemibold"
                        android:text="@string/fixed12"
                        android:textSize="14dp" />
                </LinearLayout>

                <View
                    android:id="@+id/view9"
                    android:layout_width="350dp"
                    android:layout_height="0.5dp"
                    android:layout_marginStart="45dp"
                    android:background="@drawable/back_my_profile" />

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginStart="15dp"
                      android:layout_marginTop="12dp"
                      android:orientation="horizontal">

                      <ImageView
                          android:id="@+id/order_tracking_img"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="8dp"
                          app:srcCompat="@drawable/ic_order_tracking" />

                      <TextView
                          android:id="@+id/order_tracking_txt"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginStart="10dp"
                          android:fontFamily="@font/cairosemibold"
                          android:text="@string/profile"
                          android:textSize="14dp" />
                  </LinearLayout>

                  <View
                      android:id="@+id/view1"
                      android:layout_width="350dp"
                      android:layout_height="0.5dp"
                      android:layout_marginStart="45dp"
                      android:background="@drawable/back_my_profile"
                      android:scaleType="fitXY" />

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginStart="15dp"
                      android:layout_marginTop="12dp"
                      android:orientation="horizontal">

                      <ImageView
                          android:id="@+id/pakckeging_stores_img"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="8dp"
                          app:srcCompat="@drawable/ic_online_store" />

                      <TextView
                          android:id="@+id/pakckeging_stores"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginStart="10dp"
                          android:fontFamily="@font/cairosemibold"
                          android:text="@string/profile1"
                          android:textSize="14dp" />
                  </LinearLayout>

                  <View
                      android:id="@+id/view2"
                      android:layout_width="350dp"
                      android:layout_height="0.5dp"
                      android:layout_marginStart="45dp"
                      android:background="@drawable/back_my_profile" />

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginStart="15dp"
                      android:layout_marginTop="12dp"
                      android:orientation="horizontal">

                      <ImageView
                          android:id="@+id/calculator"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="8dp"
                          app:srcCompat="@drawable/ic_settings" />

                      <TextView
                          android:id="@+id/calculator_txt"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginStart="10dp"
                          android:fontFamily="@font/cairosemibold"
                          android:text="@string/profile2"
                          android:textSize="14dp" />
                  </LinearLayout>

                  <View
                      android:id="@+id/view3"
                      android:layout_width="350dp"
                      android:layout_height="0.5dp"
                      android:layout_marginStart="45dp"
                      android:background="@drawable/back_my_profile" />

                  <View
                      android:id="@+id/view5"
                      android:layout_width="match_parent"
                      android:layout_height="1dp"
                      android:layout_marginTop="30dp"
                      android:background="@color/my_profile" />

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginStart="15dp"
                      android:layout_marginTop="30dp"
                      android:orientation="horizontal">

                      <ImageView
                          android:id="@+id/img_advertising"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="8dp"
                          app:srcCompat="@drawable/ic_path_13401" />

                      <TextView
                          android:id="@+id/advertising_txt"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginStart="10dp"
                          android:fontFamily="@font/cairosemibold"
                          android:text="@string/profile3"
                          android:textSize="14dp" />
                  </LinearLayout>

                  <View
                      android:id="@+id/view4"
                      android:layout_width="350dp"
                      android:layout_height="0.5dp"
                      android:layout_marginStart="45dp"
                      android:background="@drawable/back_my_profile" />


                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginStart="15dp"
                      android:layout_marginTop="12dp"
                      android:orientation="horizontal">

                      <ImageView
                          android:id="@+id/sell_img"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="8dp"
                          app:srcCompat="@drawable/ic_terms___conditions" />

                      <TextView
                          android:id="@+id/sell_txt"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginStart="10dp"
                          android:fontFamily="@font/cairosemibold"
                          android:text="@string/profile4"
                          android:textSize="14dp" />
                  </LinearLayout>

                  <View
                      android:id="@+id/view6"
                      android:layout_width="350dp"
                      android:layout_height="0.5dp"
                      android:layout_marginStart="45dp"
                      android:background="@drawable/back_my_profile" />

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginStart="15dp"
                      android:layout_marginTop="12dp"
                      android:orientation="horizontal">

                      <ImageView
                          android:id="@+id/apoiment_img"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="8dp"
                          app:srcCompat="@drawable/ic_contact_us" />

                      <TextView
                          android:id="@+id/apoiment_txt"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginStart="10dp"
                          android:fontFamily="@font/cairosemibold"
                          android:text="@string/profile5"
                          android:textSize="14dp" />
                  </LinearLayout>

                  <View
                      android:id="@+id/view7"
                      android:layout_width="350dp"
                      android:layout_height="0.5dp"
                      android:layout_marginStart="45dp"
                      android:background="@drawable/back_my_profile" />

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginStart="15dp"
                      android:layout_marginTop="12dp"
                      android:orientation="horizontal">

                      <ImageView
                          android:id="@+id/setting_img"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="8dp"
                          app:srcCompat="@drawable/ic_recommendation__1_" />

                      <TextView
                          android:id="@+id/setting"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginStart="10dp"
                          android:fontFamily="@font/cairosemibold"
                          android:text="@string/profile6"
                          android:textSize="14dp" />
                  </LinearLayout>

                  <View
                      android:id="@+id/view8"
                      android:layout_width="350dp"
                      android:layout_height="0.5dp"
                      android:layout_marginStart="45dp"
                      android:background="@drawable/back_my_profile" />


                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="wrap_content"
                      android:layout_marginStart="15dp"
                      android:layout_marginTop="12dp"
                      android:orientation="horizontal">

                      <ImageView
                          android:id="@+id/payment_method_img"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginTop="8dp"
                          app:srcCompat="@drawable/ic_appointment_booking" />

                      <TextView
                          android:id="@+id/payment_txt"
                          android:layout_width="wrap_content"
                          android:layout_height="wrap_content"
                          android:layout_marginStart="10dp"
                          android:fontFamily="@font/cairosemibold"
                          android:text="@string/profile7"
                          android:textSize="14dp" />
                  </LinearLayout>

                  <View
                      android:id="@+id/view99"
                      android:layout_width="350dp"
                      android:layout_height="0.5dp"
                      android:layout_marginStart="45dp"
                      android:background="@drawable/back_my_profile" />

              </LinearLayout>
            </LinearLayout>
    </ScrollView>
</LinearLayout>