<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/transparent_color">

        <ImageView
            android:id="@+id/img_back"
            android:layout_width="25dp"
            android:layout_height="23dp"
            android:layout_gravity="left"
            android:layout_marginLeft="20dp"
            app:srcCompat="@drawable/backk"
                android:contentDescription="@string/about_hama3" />

        <TextView
            android:id="@+id/txt_toolbar"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_gravity="center"
            android:fontFamily="@font/cairobold"
            android:text="@string/Contact_Us"
            android:textColor="@color/filter_by"
            android:textSize="20sp"/>
    </androidx.appcompat.widget.Toolbar>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView106"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="10dp"
                android:fontFamily="@font/cairobold"
                android:text="@string/Contact_Us12"
                android:textColor="@color/blackcolor1"
                android:textSize="18sp" />

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:id="@+id/imageView18"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:srcCompat="@drawable/camm" />

                <ImageView
                    android:id="@+id/imageView19"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="@+id/imageView18"
                    app:layout_constraintStart_toStartOf="@+id/imageView18"
                    android:layout_marginStart="45dp"
                    android:layout_marginTop="50dp"
                    app:layout_constraintBottom_toBottomOf="@+id/imageView18"
                    app:srcCompat="@drawable/ic_icon_awesome_camera" />


                <EditText
                    android:id="@+id/edt_email"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/imageView18"
                    android:hint="@string/Full_Name"
                    android:inputType="textPersonName"
                    android:textColorHint="@color/text"
                    android:padding="10dp"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />

                <EditText
                    android:id="@+id/edt_email11"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    android:hint="@string/email"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_email"
                    android:inputType="textPersonName"
                    android:padding="10dp"
                    android:textColorHint="@color/text"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />

                <EditText
                    android:id="@+id/edt_email111"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_email11"
                    android:hint="@string/Phone_Number"
                    android:inputType="textPersonName"
                    android:padding="10dp"
                    android:textColorHint="@color/text"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />

                <EditText
                    android:id="@+id/editText18"
                    android:layout_width="match_parent"
                    android:layout_height="170dp"
                    android:layout_gravity="center"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="20dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_email111"
                    android:gravity="start"
                    android:hint="@string/complint51"
                    android:inputType="text"
                    android:padding="10dp"
                    android:paddingStart="10dp"
                    android:textColorHint="@color/text" />

                <TextView
                    android:id="@+id/textView107"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/cairobold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/editText18"
                    android:text="@string/Contact_Us12"
                    android:textColor="@color/blackcolor1"
                    android:textSize="18sp" />


                <EditText
                    android:id="@+id/edt_email14"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView107"
                    android:hint="@string/ID_No"
                    android:inputType="textPersonName"
                    android:textColorHint="@color/text"
                    android:padding="10dp"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:gravity="center"
                    android:id="@+id/up"
                    android:layout_marginTop="20dp"
                    android:layout_marginEnd="30dp"
                    android:layout_marginStart="30dp"
                    app:layout_constraintStart_toStartOf="parent"
                    android:background="@drawable/ic_rectangle_3294"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_email14">


                    <ImageView
                        android:id="@+id/imageView6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/ic_id" />

                    <TextView
                        android:id="@+id/textView12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:textColor="@color/ghhg"
                        android:layout_marginTop="10dp"
                        android:gravity="center"
                        android:text="@string/Capture_imagejs" />
                </LinearLayout>

                <EditText
                    android:id="@+id/edt_email889"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/up"
                    android:hint="@string/Gender"
                    android:inputType="textPersonName"
                    android:textColorHint="@color/text"
                    android:padding="10dp"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />



                <EditText
                    android:id="@+id/edt_email89"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_email889"
                    android:hint="@string/Date_of_Birth"
                    android:inputType="textPersonName"
                    android:textColorHint="@color/text"
                    android:padding="10dp"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />


                <EditText
                    android:id="@+id/edt_email899"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_email89"
                    android:hint="@string/Personal_Phone_Number"
                    android:inputType="textPersonName"
                    android:textColorHint="@color/text"
                    android:padding="10dp"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />

                <EditText
                    android:id="@+id/edt_emai21"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    android:hint="@string/Store_Phone_Number"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_email899"
                    android:inputType="textPersonName"
                    android:padding="10dp"
                    android:textColorHint="@color/text"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />

                <TextView
                    android:id="@+id/textView109"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="10dp"
                    android:fontFamily="@font/cairobold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_emai21"
                    android:text="@string/Store_Informations"
                    android:textColor="@color/blackcolor1"
                    android:textSize="18sp" />

                <LinearLayout
                    android:id="@+id/linearLayout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="20dp"
                    android:layout_weight="1"
                    android:layout_marginEnd="30dp"
                    android:background="@drawable/ic_rectangle_3294"
                    android:gravity="center"
                    android:orientation="vertical"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView109">

                    <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/up_load" />

                    <TextView
                        android:id="@+id/textView11"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:textSize="14sp"
                        android:fontFamily="@font/cairosemibold"
                        android:gravity="center"
                        android:text="@string/Clicck"
                        android:textColor="@color/ghhg" />
                </LinearLayout>
                <EditText
                    android:id="@+id/edt_emai15"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    android:hint="@string/License_expiry_date"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout"
                    android:inputType="textPersonName"
                    android:padding="10dp"
                    android:textColorHint="@color/text"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />


                <LinearLayout
                    android:id="@+id/linearLayout2"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="20dp"
                    android:layout_weight="1"
                    android:layout_marginEnd="30dp"
                    android:background="@drawable/ic_rectangle_3294"
                    android:gravity="center"
                    android:orientation="vertical"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_emai15">

                    <ImageView
                        android:id="@+id/imageView55"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/up_load" />

                    <TextView
                        android:id="@+id/textView111"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:textSize="14sp"
                        android:fontFamily="@font/cairosemibold"
                        android:gravity="center"
                        android:text="@string/Capture_for_lease"
                        android:textColor="@color/ghhg" />
                </LinearLayout>
                <EditText
                    android:id="@+id/edt_emai115"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    android:hint="@string/License_expiry_date"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
                    android:inputType="textPersonName"
                    android:padding="10dp"
                    android:textColorHint="@color/text"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />


                <LinearLayout
                    android:id="@+id/linearLayout3"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="20dp"
                    android:layout_weight="1"
                    android:layout_marginEnd="30dp"
                    android:background="@drawable/ic_rectangle_3294"
                    android:gravity="center"
                    android:orientation="vertical"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_emai115">

                    <ImageView
                        android:id="@+id/imageView64"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:srcCompat="@drawable/up_load" />

                    <TextView
                        android:id="@+id/textView112"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:textSize="14sp"
                        android:fontFamily="@font/cairosemibold"
                        android:gravity="center"
                        android:text="@string/Cliccck"
                        android:textColor="@color/ghhg" />
                </LinearLayout>


                <EditText
                    android:id="@+id/edt_emai5"
                    android:layout_width="360dp"
                    android:layout_height="45dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/back_ground_edit_text"
                    android:ems="10"
                    android:hint="@string/License_expiry_date"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/linearLayout3"
                    android:inputType="textPersonName"
                    android:padding="10dp"
                    android:textColorHint="@color/text"
                    android:paddingStart="20dp"
                    android:textSize="15sp" />


                <Button
                    android:id="@+id/btn_send"
                    android:layout_width="256dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/edt_emai5"
                    android:background="@drawable/back_ground_shadow"
                    android:fontFamily="@font/cairobold"
                    android:text="@string/send22"
                    android:textColor="@color/white"
                    android:textSize="16sp" />
            </androidx.constraintlayout.widget.ConstraintLayout>



        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</LinearLayout>