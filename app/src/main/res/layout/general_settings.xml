<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/transparent_color">

        <ImageView
            android:id="@+id/toolbar_img"
            android:layout_width="22dp"
            android:layout_height="23dp"
            android:layout_gravity="start"
            app:srcCompat="@drawable/backk" />

        <TextView
            android:id="@+id/home1"
            android:layout_width="wrap_content"
            android:layout_height="46dp"
            android:layout_gravity="center"
            android:fontFamily="@font/cairobold"
            android:text="@string/Store_Rate1"
            android:textColor="@color/blackcolor1"
            android:textSize="20dp" />
    </androidx.appcompat.widget.Toolbar>
    <View
        android:id="@+id/view969"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/blackcolor" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView53"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="10dp"
            app:srcCompat="@drawable/ic_subject" />

        <TextView
            android:id="@+id/language_setting"
            android:layout_width="wrap_content"
            android:fontFamily="@font/cairosemibold"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:textColor="@color/my_profile"
            android:layout_marginStart="20dp"
            android:text="@string/Language_Settings" />
    </LinearLayout>

    <View
        android:id="@+id/view12"
        android:layout_width="310dp"
        android:background="@drawable/back_my_profile"
        android:layout_marginTop="5dp"
        android:layout_marginStart="60dp"
        android:layout_height="0.5dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView55"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="10dp"
            app:srcCompat="@drawable/ic_notification__2_" />

        <TextView
            android:id="@+id/textView81"
            android:layout_width="wrap_content"
            android:fontFamily="@font/cairosemibold"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:textColor="@color/my_profile"
            android:layout_marginStart="20dp"
            android:text="@string/Language_Settings2" />
    </LinearLayout>

    <View
        android:id="@+id/view11"
        android:layout_width="310dp"
        android:background="@drawable/back_my_profile"
        android:layout_marginTop="5dp"
        android:layout_marginStart="60dp"
        android:layout_height="0.5dp" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView54"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="10dp"
            app:srcCompat="@drawable/ic_path_13411" />

        <TextView
            android:id="@+id/textView82"
            android:layout_width="wrap_content"
            android:fontFamily="@font/cairosemibold"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:textColor="@color/my_profile"
            android:layout_marginStart="20dp"
            android:text="@string/Language_Settings3" />
    </LinearLayout>

    <View
        android:id="@+id/view13"
        android:layout_width="310dp"
        android:background="@drawable/back_my_profile"
        android:layout_marginTop="5dp"
        android:layout_marginStart="60dp"
        android:layout_height="0.5dp" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView56"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginStart="10dp"
            app:srcCompat="@drawable/ic_icon_feather_link_2" />

        <TextView
            android:id="@+id/textView83"
            android:layout_width="wrap_content"
            android:fontFamily="@font/cairosemibold"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:textColor="@color/my_profile"
            android:layout_marginStart="20dp"
            android:text="@string/Language_Settings7" />
    </LinearLayout>

    <View
        android:id="@+id/view14"
        android:layout_width="310dp"
        android:background="@drawable/back_my_profile"
        android:layout_marginTop="5dp"
        android:layout_marginStart="60dp"
        android:layout_height="0.5dp" />
</LinearLayout>